orjson = "^3.8.7"
py-grpc-prometheus = {git = "https://github.com/sollof/py-grpc-prometheus.git", rev = "94e49c7"}

[build-system]
requires = ["poetry>=0.12"]
build-backend = "poetry.masonry.api"
